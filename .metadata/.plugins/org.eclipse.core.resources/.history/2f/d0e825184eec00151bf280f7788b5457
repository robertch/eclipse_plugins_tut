	@Override
	public boolean hasChildren(Object element) {
		if(element instanceof Map){
			return !((Map)element).isEmpty();
		}else if(element instanceof Map.Entry){
			return hasChildren(((MapEntry)element).getValue())
		}else if(){
			
		}else{
			return false;
